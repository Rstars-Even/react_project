{
    "files.autoSave": "off",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "tsconfig.json": "tsconfig..,composer.json,phpunit,vite.config.js,index.html,.yml,.config.js,.gen.ts,.d.ts,package.json,tsconfig.node,json,.gitignore,electron.vite.config.js,update.md,vite.config.ts,tsconfig.app.json,tsconfig.node.json,README.md,eslintrc.cjs,eslint.config.js,prettierrc.js,.prettierrc",
        "pnpm-lock.yaml": "stylelintrc.js,.tslintignore,.eslintignore,prettierignore,.prettierrc.yaml,.npmrc,editorconfig,yarn.*",
    },
    "files.exclude": {
        ".yarn": true,
        "resources": true,
        "build": true,
        "pnpm-lock.yaml": true,
        ".vscode": false,
        "node_modules": true,
        "dist": true,
        "icons.iconset": true,
        "icon.iconset": true
    },
    "html.format.wrapAttributes": "preserve-aligned",
    "editor.wordWrap": "on",
    "workbench.editor.wrapTabs": false,
    "scssFormatter.singleQuote": false,
    "eslint.problems.shortenToSingleLine": false,
    "editor.foldingImportsByDefault": true
}